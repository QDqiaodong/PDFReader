    android:id="@+id/main_presentation_layout_root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/epd_menu_bottom_set_bg_color"
    android:orientation="vertical">

    <include layout="@layout/presentation_menu_layout"/>

    <FrameLayout
        android:id="@+id/layout_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
    </FrameLayout>

</LinearLayout>



